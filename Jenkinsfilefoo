node() {
        stage('Checkout') {
        checkout scm
        }

        def WORK_DIR=pwd()
        def MAVEN_GOAL='verify'

        stage('Configure deploy') {
           echo "env.GIT_BRANCH:" + env.GIT_BRANCH
           if (env.GIT_BRANCH in ['master', 'maven-3.8.x', 'maven-3.9.x']){
               MAVEN_GOAL='deploy'
           }
        }
        echo "MAVEN_GOAL: $MAVEN_GOAL"
}
