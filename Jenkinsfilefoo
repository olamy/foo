@Library('mylibrary') _

pipeline {
  agent any
    stages {
      stage("Parallel Stage") {
        parallel {    
          stage('Demo') {
            steps {
              echo 'Hello, world'
              sayHello 'Dave'
            }
          }
          stage('Demo2') {
            steps {
              echo 'Hello, world2'

            }
          }          
        }  
    }       
  }
}
